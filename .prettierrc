{
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 100,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^@nest(.*)$",
    "^@app\/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^./entities\/(.*)$",
    "^..\/(.*)$",
    "^.\/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "classProperties",
    "decorators-legacy"
  ]
}